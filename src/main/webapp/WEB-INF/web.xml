<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	id="WebApp_ID" version="4.0">
	<display-name>CityManager</display-name>
	<servlet>
		<servlet-name>servletparameters</servlet-name>
		<servlet-class>day2.citymanager.servlets.ServletParameters</servlet-class>
		<init-param>
			<param-name>secretCode</param-name>
			<param-value>111-11-1111</param-value>
		</init-param>
	</servlet>

	<servlet-mapping>
		<servlet-name>servletparameters</servlet-name>
		<url-pattern>/servletparameters.do</url-pattern>
	</servlet-mapping>

	<context-param>
		<param-name>dbuserid</param-name>
		<param-value>root</param-value>
	</context-param>

	<context-param>
		<param-name>dbuserpwd</param-name>
		<param-value>admin</param-value>
	</context-param>

	<context-param>
		<param-name>dbinitcat</param-name>
		<param-value>bookmanager</param-value>
	</context-param>


	<context-param>
		<param-name>initialSeed</param-name>
		<param-value>2</param-value>
	</context-param>

	<!-- <session-config> <session-timeout>20</session-timeout> <cookie-config> 
		<max-age>21</max-age> </cookie-config> <tracking-mode>COOKIE</tracking-mode> 
		</session-config> -->
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>